

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
enum Prioridade {
  baixa
  media
  alta
}


model Member {
  id       Int    @id @default(autoincrement())
  email    String @unique
  name     String

}


model Tarefa {
  id Int @id @default(autoincrement())
  name String
  decricao String @db.VarChar(140)
  finalizada Boolean @default(false)
  data_termino DateTime
  prioridade  Prioridade 
}

